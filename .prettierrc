{
  "plugins": ["prettier-plugin-tailwindcss"],
  "printWidth": 80,
  "tabWidth": 2,
  "trailingComma": "all",
  "endOfLine": "lf",
  "semi": true,
  "useTabs": false,
  "singleAttributePerLine": true,
  "singleQuote": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always"
}
